{
  "name": "arxiv-cosci",
  "version": "0.4.0",
  "description": "ArXiv AI Co-Scientist - Scientific Intelligence Platform",
  "private": true,
  "scripts": {
    "start": "./scripts/start.sh",
    "stop": "./scripts/stop.sh",
    "dev": "cd apps/web && npm run dev",
    "build": "cd apps/web && npm run build",
    "api": "poetry run uvicorn apps.api.main:app --reload --host 0.0.0.0 --port 8000",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "test": "poetry run pytest tests/ -v",
    "test:startup": "./scripts/test-startup.sh",
    "test:integration": "poetry run pytest tests/test_startup_integration.py -v",
    "lint": "poetry run ruff check .",
    "format": "poetry run ruff format ."
  },
  "keywords": [
    "arxiv",
    "scientific-research",
    "ai",
    "knowledge-graph",
    "citations"
  ],
  "author": "ArXiv CoSci Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}